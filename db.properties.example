# Arquivo de exemplo para configuração do banco de dados
# Copie este arquivo para db.properties e configure com suas credenciais

# URL de conexão com o banco MySQL
dburl=jdbc:mysql://localhost:3306/livraria?useSSL=false&serverTimezone=UTC

# Usuário do banco de dados
user=root

# Senha do banco de dados
password=123456

